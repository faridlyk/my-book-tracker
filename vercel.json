{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
         }
    ],
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/main.py"
        }
    ]
}